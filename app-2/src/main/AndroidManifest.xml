<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.feicui.com.main">

    <application
        android:allowBackup="true"
        android:icon="@drawable/a1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.feicui.com.main.MainActivity"></activity>
        <activity android:name="cn.feicui.com.main.AnimationActivity" />
        <activity android:name="cn.feicui.com.main.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data android:name="com.google.android.gms.version" />

        <activity android:name="cn.feicui.com.main.TelmsgActivity"></activity>
    </application>

</manifest>